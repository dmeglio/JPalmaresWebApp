@model IEnumerable<JPalmaresWebApp.Models.Vittorie>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Idtrofeo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Stagione)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Commenti)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Ordstagione)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Ordprogressivo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Allenatore)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Idtrofeo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stagione)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Commenti)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ordstagione)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ordprogressivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Allenatore)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
